{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/karpovigorok/laravel-comment"
		}
	],
	"require": {
		"laravel/framework": "5.5.*",
		"intervention/image": "2.4.2",
        "laravel/cashier":"~7.0",
        "laravel/socialite": "~3.0",
		"actuallymab/laravel-comment": "dev-sub-anonymous-l5.5",
		"videojs/video.js": "^7.1",
		"arrilot/laravel-widgets": "^3.12",
		"cybercog/laravel-love": "^5.1",
		"pbmedia/laravel-ffmpeg": "1.3",
		"pion/laravel-chunk-upload": "^1.2",
		"blueimp/jquery-file-upload": "^9.22",
		"davejamesmiller/laravel-breadcrumbs": "4.x",
		"belphemur/laravel-gettext": "6.x",
		"infyomlabs/swagger-generator": "dev-master",
		"jlapp/swaggervel": "dev-master",

		"arrilot/laravel-api-generator": "^1.1",
		"arcanedev/seo-helper": "1.4.x",
		"arcanedev/laravel-seo": "^0.6.0",
		"vlucas/phpdotenv": "2.*"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0"
	},
	"autoload": {
		"classmap": [
			"database",
			"app/Http/Controllers",
			"app/Models",
			"app/Libraries"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
	},
	"config": {
		"preferred-install": "dist"
	}
}
